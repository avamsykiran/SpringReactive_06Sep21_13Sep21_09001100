package com.cts.rjd.ui;

import com.cts.rjd.service.NumberSeriesGeneratorReactiveService;
import com.cts.rjd.service.NumberSeriesGeneratorReactiveService2;

public class App03 {

	public static void main(String[] args) {

		NumberSeriesGeneratorReactiveService2 nss = new NumberSeriesGeneratorReactiveService2();
		
		nss.generateSeries(10, 25).subscribe(
				System.out::println, 
				Throwable::getMessage, 
				() -> System.out.println("Job1 Accomplished")
				);
		
		nss.generateSeries(100, 250).subscribe(
				System.out::println, 
				Throwable::getMessage, 
				() -> System.out.println("Job1 Accomplished")
				);
		
		System.out.println("Hello All of you!!");
	}

}
